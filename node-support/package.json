{
  "name": "cloudstate",
  "version": "0.0.5",
  "description": "Support for implementing CloudState functions",
  "keywords": [
    "serverless",
    "cloudstate",
    "event-sourcing",
    "crdt"
  ],
  "homepage": "https://github.com/cloudstateio/cloudstate",
  "bugs": {
    "url": "https://github.com/cloudstateio/cloudstate"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "James Roper",
    "email": "jroper2@gmail.com",
    "url": "https://jazzy.id.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudstateio/cloudstate",
    "directory": "src/node-support"
  },
  "engineStrict": true,
  "engines": {
    "node": "~12"
  },
  "files": [
    "proto",
    "protobuf-bundle.js",
    "protobuf-bundle.d.ts",
    "src",
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@grpc/proto-loader": "^0.5.6",
    "debug": "^4.1.1",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.11.0",
    "json-stable-stringify": "^1.0.1",
    "minimist": ">=0.2.1",
    "mkdirp": "^0.5.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "test": "^0.6.0",
    "unzipper": "^0.9.15"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.3",
    "mocha": "^6.1.4",
    "npm-watch": "^0.6.0",
    "nyc": "^14.1.1"
  },
  "bin": {
    "download-protoc": "bin/download-protoc.js",
    "compile-descriptor": "bin/compile-descriptor.js"
  },
  "watch": {
    "jsdoc": {
      "patterns": [
        "src/**/*.js",
        "index.js",
        "README.md",
        "jsdoc.json"
      ],
      "ignore": "apidocs"
    }
  },
  "scripts": {
    "print-version": "echo $npm_package_version",
    "watch-jsdoc": "npm-watch jsdoc",
    "jsdoc": "jsdoc -c jsdoc.json",
    "test": "mocha --recursive",
    "prepare": "bin/prepare.sh",
    "postinstall": "bin/download-protoc.js"
  }
}
